<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Pressed -->
    <item android:drawable="@drawable/button_shape_pressed" android:state_pressed="true" />

    <!-- focused -->
    <item android:drawable="@drawable/button_not_pressed_drawable" android:state_focused="true" />

    <!-- selected -->
    <item android:drawable="@drawable/button_shape_selected" android:state_selected="true" />

    <!-- default -->
    <item android:drawable="@drawable/button_shape_normal" />


</selector>